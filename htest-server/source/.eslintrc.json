{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "arrow-parens": ["error", "always"]
    }
}
